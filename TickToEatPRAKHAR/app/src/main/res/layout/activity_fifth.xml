<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FifthActivity">

    <CheckBox
        android:id="@+id/radiofifth4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/fifth2"
        android:layout_alignTop="@+id/radiofifth3"
        android:onClick="getItem5"
        android:text="Apple Pie"
        android:layout_alignLeft="@+id/fifth2"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/fifth2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/fifth1"
        android:layout_marginEnd="68dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/fifth2"
        android:layout_alignParentRight="true"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/fifth3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/fifth1"
        android:layout_marginTop="169dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/fifth3"
        android:layout_alignLeft="@+id/fifth1"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/fifth4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignLeft="@+id/fifth2"
        android:layout_alignStart="@+id/fifth2"
        android:layout_alignTop="@+id/fifth3"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/fifth4"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/fifth1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="29dp"
        app:srcCompat="@drawable/fifth1"
        tools:background="#60f5c9c9"
        android:layout_alignParentLeft="true"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="104dp"
        android:onClick="goBack5"
        android:text="BACK" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/second2"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:text="DESSERT"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiofifth2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/fifth3"
        android:layout_marginTop="129dp"
        android:onClick="getItem5"
        android:text="Custard"
        android:layout_alignLeft="@+id/fifth3"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiofifth1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/radiofifth4"
        android:layout_alignTop="@+id/radiofifth2"
        android:onClick="getItem5"
        android:text="Rice Pudding"
        android:layout_alignLeft="@+id/radiofifth4"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiofifth3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/fifth3"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/fifth3"
        android:layout_marginBottom="208dp"
        android:onClick="getItem5"
        android:text="Sorbet"
        tools:ignore="RtlCompat" />

</RelativeLayout>