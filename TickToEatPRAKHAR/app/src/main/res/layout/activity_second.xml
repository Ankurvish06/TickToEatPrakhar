<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondActivity">

    <CheckBox
        android:id="@+id/radiosecond4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/radiosecond3"
        android:layout_marginEnd="58dp"
        android:onClick="getItem2"
        android:text="Cheese Balls"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiosecond5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button2"
        android:layout_toLeftOf="@+id/button2"
        android:layout_toStartOf="@+id/button2"
        android:onClick="getItem2"
        android:text="Chicken Satay"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/second2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/second1"
        android:layout_marginEnd="68dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/second2"
        android:layout_marginRight="68dp"
        android:layout_alignParentRight="true"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/second3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/second1"
        android:layout_marginTop="169dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/second3"
        android:layout_alignLeft="@+id/second1"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/second4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignStart="@+id/second2"
        android:layout_alignTop="@+id/second3"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/second4"
        android:layout_alignLeft="@+id/second2"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/second5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/second3"
        android:layout_marginBottom="106dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/second5"
        android:layout_alignLeft="@+id/second3"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/second6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignStart="@+id/second2"
        android:layout_alignTop="@+id/second5"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/second6"
        android:layout_alignLeft="@+id/second2"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/second1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="29dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/second1"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="41dp"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="19dp"
        android:onClick="goBack2"
        android:text="BACK" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/second2"
        android:layout_centerHorizontal="true"
        android:text="STARTERS"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

    <CheckBox
        android:id="@+id/radiosecond2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/radiosecond4"
        android:layout_alignStart="@+id/radiosecond4"
        android:layout_alignTop="@+id/radiosecond1"
        android:onClick="getItem2"
        android:text="Fried chilli chicken"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiosecond1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/radiosecond5"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/radiosecond5"
        android:layout_marginTop="127dp"
        android:onClick="getItem2"
        android:text="Kakori Kebabs"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiosecond3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/radiosecond5"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/radiosecond5"
        android:layout_marginBottom="205dp"
        android:onClick="getItem2"
        android:text="Paneer Tikka"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiosecond6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/radiosecond4"
        android:layout_alignStart="@+id/radiosecond4"
        android:layout_alignTop="@+id/radiosecond5"
        android:onClick="getItem2"
        android:text="Tangri Kawab"
        tools:ignore="RtlCompat" />

</RelativeLayout>