<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ThirdActivity">

    <CheckBox
        android:id="@+id/radiothird4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/radiothird3"
        android:layout_toEndOf="@+id/textView3"
        android:onClick="getItem3"
        android:text="Chicken Wings"
        android:layout_toRightOf="@+id/textView3"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/third2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/third1"
        android:layout_marginEnd="68dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/third2"
        android:layout_alignParentRight="true"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/third3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/third1"
        android:layout_marginStart="0dp"
        android:layout_marginTop="169dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/third3"
        android:layout_marginLeft="0dp"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/third4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignStart="@+id/third2"
        android:layout_alignTop="@+id/third3"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/third4"
        android:layout_alignLeft="@+id/third2"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/third1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="29dp"
        android:background="#60f5c9c9"
        app:srcCompat="@drawable/third1"
        android:layout_alignParentLeft="true"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/third2"
        android:layout_alignLeft="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:text="APPETIZER"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiothird2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/radiothird4"
        android:layout_alignTop="@+id/radiothird1"
        android:onClick="getItem3"
        android:text="Pepper Poppers"
        android:layout_alignRight="@+id/radiothird4"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiothird3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/radiothird1"
        android:layout_marginBottom="213dp"
        android:onClick="getItem3"
        android:text="Homemade Guacamole"
        android:layout_alignLeft="@+id/radiothird1"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/radiothird1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="129dp"
        android:onClick="getItem3"
        android:text="Buffalo Chicken Dip"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="47dp"
        android:onClick="goBack3"
        android:text="BACK" />

</RelativeLayout>